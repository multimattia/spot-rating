{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/loadplaylist\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/logout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/playlists\\/([^/]+?)\\/([^/]+?)\\/tracks\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/save\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/createplaylist\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/fetchPlaylist\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/spotify\\/callback\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/spotify\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/playlists\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}